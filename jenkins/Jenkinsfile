node {
	stage('Clean Workspace') {
		deleteDir()
	}
	stage ('Git Clone') {
		checkout scm
	}
	stage ('Build') {
		bat "echo 'Building...'"
		bat 'mvn clean package'
	}
	stage ('Build Docker Image') {
		
		docker.withServer('tcp://192.168.0.103:2375') {
		docker.build("tyt/myspringapp:8.111", "./docker")
    }
}
